                {
                    "name": "DB_NAME",
                    "value": "cragheads-db"
                },
                {
                    "name": "DB_CONN_STRING",
                    "value": "mongodb+srv://lvanleynseele:byGO9fysygLDHY3M@cragheads-db.gelu5.mongodb.net/?retryWrites=true&w=majority&appName=cragheads-db"
                },
                {
                    "name": "PORT",
                    "value": "3010"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
